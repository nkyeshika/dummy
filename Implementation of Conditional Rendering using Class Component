import React, { Component } from 'react';

export class Lab extends Component {
  constructor(props) {
    super(props);
    this.state = {
      showGreeting: false,
    };
  }
  render() {
    return (
      <div>
        {this.state.showGreeting ? <h1>Welcome to the Site!</h1> : <h1>Have a great day!</h1>}
      </div>
    );
  }
}
export default Lab;
